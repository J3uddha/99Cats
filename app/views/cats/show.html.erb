<h1 align="center">99Cats - "<%= @cat.name %>"</h1>

<table>
  <tr>
    <td>Name: </td>
    <td> <%= @cat.name %></td>
  </tr>
  <tr>
    <td>Birthday:</td>
    <td><%= @cat.age %></td>
  </tr>
  <tr>
    <td>Color:</td>
      <td><%= @cat.color %></td>
  </tr>
  <tr>
    <td>Sex:</td>
      <td><%= @cat.sex %></td>
  </tr>
  <tr>
    <td>Description:</td>
      <td><%= @cat.description %></td>
  </tr>
</table>

<p><%= link_to 'Edit this cat!', edit_cat_url(@cat) %></p>
<p><%= link_to 'Back to all cats', cats_url %></p>
